<rml>

    <head>
        <link type="text/template" href="../../../core/assets/ui/tmp-menu-page.rml" />
        <title>Mod Options</title>
    </head>

    <body template="tmp-menu-page">
        <div id="menu-title-bar">
            <span id="title">Mod Options</span>
        </div>
        <div data-for="mod : mods">
            <button data-event-click="onNavigate('options-' + mod.id)" data-if="mod.hasModOptions">{{mod.name}}</button>
        </div>
    </body>
</rml>